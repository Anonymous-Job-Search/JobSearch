{
  "name": "@anonymous-job-search/web",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "lint": "npm run typescript && npm run eslint && npm run prettier && npm run stylelint",
    "lint:fix": "npm run typescript && npm run eslint:fix && npm run prettier:fix && npm run stylelint:fix",
    "typescript": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "eslint": "eslint . --ext .{vue,js,ts,tsx} --ignore-path .gitignore",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier": "prettier **/*.{html,vue,js,ts,tsx,spec.ts} --ignore-path .gitignore --check",
    "prettier:fix": "prettier **/*.{html,vue,js,ts,tsx,spec.ts} --ignore-path .gitignore --write",
    "stylelint": "stylelint **/*.scss --ignore-path .gitignore",
    "stylelint:fix": "npm run stylelint -- --fix",
    "test": "npm run test:unit",
    "test:coverage": "npm run test:unit:coverage",
    "test:unit": "vitest run --environment jsdom",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "vitest watch --environment jsdom"
  },
  "dependencies": {
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "~4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.0.22"
  }
}
